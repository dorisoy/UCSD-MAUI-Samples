<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiDataBinding.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiDataBinding"
    xmlns:views="clr-namespace:MauiDataBinding.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="BasicBinding"
        ContentTemplate="{DataTemplate views:BasicXamlBindingPage}"
        Icon="appiconfg.png"
        Route="BasicXamlBindingPage" />
    <ShellContent
        Title="ObjectBinding"
        Icon="appiconfg.png"
        ContentTemplate="{DataTemplate views:ObjectBindingPage}"
        Route="ObjectBindingPage" />

</Shell>
