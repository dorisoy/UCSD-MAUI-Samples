<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiDevices.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiDevices.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Device Info"
        ContentTemplate="{DataTemplate local:DeviceInformationPage}"
        Route="DeviceInformationPage" />
    <ShellContent
        Title="Device Display"
        ContentTemplate="{DataTemplate local:DeviceDisplayPage}"
        Route="DeviceDisplayPage" />
    <ShellContent
        Title="Battery Info"
        ContentTemplate="{DataTemplate local:BatteryPage}"
        Route="BatteryPage" />

</Shell>
