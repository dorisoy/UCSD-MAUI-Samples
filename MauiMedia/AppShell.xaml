<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiMedia.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiMedia.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Barcode Reading"
        Icon="appiconfg.png"
        ContentTemplate="{DataTemplate local:BarCodesReadingPage}"
        Route="BarCodesReadingPage" />
    <ShellContent
        Title="Audio Media player"
        Icon="appiconfg.png"
        ContentTemplate="{DataTemplate local:SoundAndMusic}"
        Route="SoundAndMusic" />
</Shell>
