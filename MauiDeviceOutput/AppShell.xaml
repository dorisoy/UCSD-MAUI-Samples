<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiDeviceOutput.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiDeviceOutput.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Flash Light"
        ContentTemplate="{DataTemplate views:FlashLightPage}"
        Route="FlashLightPage" />
    <ShellContent
        Title="Haptic Feedback"
        ContentTemplate="{DataTemplate views:HapticFeedbackPage}"
        Route="HapticFeedbackPage" />
    <ShellContent
        Title="Vibration "
        ContentTemplate="{DataTemplate views:VibrationPage}"
        Route="VibrationPage" />

</Shell>
